    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "eventemitter3": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "fbjs": "^0.8.1",
    "file-loader": "^0.8.4",
    "fs-extra": "^0.30.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "intl": "^1.0.x",
    "iscroll": "^5.2.0",
